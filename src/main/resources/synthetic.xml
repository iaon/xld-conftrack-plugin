<?xml version='1.0' encoding='UTF-8'?>
<synthetic xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd" xmlns="http://www.xebialabs.com/deployit/synthetic"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <type type="conftrack.Repository" extends="udm.Configuration" virtual="true">
    </type>

    <type type="conftrack.svnRepository" extends="conftrack.Repository">
        <property name="URL" required="true" kind="string"/>
        <property name="username" required="false" kind="string"/>
        <property name="password" required="false" kind="string" password="true"/>
    </type>

    <type type="conftrack.WorkingCopy" extends="generic.Container" virtual="true">
    </type>

    <type type="conftrack.svnWorkingCopy" extends="conftrack.WorkingCopy">
        <property name="host" kind="ci" referenced-type="overthere.Host" as-containment="true"/>
        <property name="Repository" required="true" kind="ci" referenced-type="conftrack.svnRepository"/>
        <property name="WorkingDirectory" required="true" kind="string">
        <property name="svnPath" required="true" kind="string">
        <method name="checkout"/>
    </type>
    
    <type type="conftrack.TrackItem" extends="udm.BaseDeployedArtifact" deployable-type="conftrack.TrackedItem" container-type="conftrack.WorkingCopy">
        <generate-deployable type="conftrack.TrackedItem" extends="udm.BaseDeployableFileArtifact"/>
    </type>

</synthetic>
